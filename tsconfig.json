    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "declaration": true,
    "rootDir": "src",
    "baseUrl": "./src",
    "outDir": "dist"
  },
  "include": [
    "src",
  ]
